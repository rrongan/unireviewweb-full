    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.1.1",
    "eslint-plugin-json": "^1.2.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^0.4.5",
